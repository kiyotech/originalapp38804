          <%= image_tag resume.image, class: "item-img" %>
          <%# if item.order.present? %>
            <%# <div class='sold-out'>
              <span>Sold Out!!</span>
            </div> %>
          <%# end %>
        </div>
        <div class='item-info'>
          <h3 class='item-name'>
            <%= resume.title %>
          </h3>
          <div class='item-price'>
            <span><%= resume.price.to_s(:delimited) %>å††</span>
            <div class='star-btn'>
              <%= image_tag "star.png", class:"star-icon" %>
              <span class='star-count'>0</span>
            </div>
          </div>
        </div>
        <% end %>
      </li>